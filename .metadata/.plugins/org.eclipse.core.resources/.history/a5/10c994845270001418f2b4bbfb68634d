

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		StringBuffer sb = new StringBuffer();
		sb.append("1+1=");
		char[] rs = sb.toString().toCharArray();
		for(Character c : rs){
			
			System.out.println(c);
		}
		
		
		System.out.println(sb);
	}

}
